{
  "rules": {
    "arrow-return-shorthand": true,
    "callable-types": true,
    "class-name": true,
    "comment-format": ["check-space", true],
    "curly": false,
    "forin": true,
    "import-blacklist": ["rxjs", true],
    "interface-name": false,
    "interface-over-type-literal": true,
    "label-position": true,
    "max-classes-per-file": false,
    "member-access": false,
    "member-ordering": [
      {
        "order": [
          "instance-field",
          "instance-method",
          "static-field",
          "static-method"
        ]
      },
      true
    ],
    "no-arg": true,
    "no-bitwise": true,
    "no-console": ["debug", "info", "log", "time", "timeEnd", "trace", true],
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-super": true,
    "no-duplicate-variable": true,
    "no-empty": false,
    "no-empty-interface": true,
    "no-eval": true,
    "no-implicit-dependencies": false,
    "no-inferrable-types": ["ignore-params", true],
    "no-misused-new": true,
    "no-non-null-assertion": true,
    "no-shadowed-variable": false,
    "no-string-literal": false,
    "no-string-throw": true,
    "no-submodule-imports": false,
    "no-switch-case-fall-through": true,
    "no-unnecessary-initializer": true,
    "no-unused-expression": true,
    "no-unused-variable": true,
    "no-use-before-declare": false,
    "no-var-keyword": true,
    "object-literal-sort-keys": false,
    "prefer-const": true,
    "radix": false,
    "triple-equals": ["allow-null-check", true],
    "unified-signatures": true,
    "variable-name": false
  }
}
